					"properties": {
						"action_timeout": 180,
						"content": "drop table lc_inventory_json;\ncreate table lc_inventory_json ( my_json clob, constraint inv_chk check ( my_json IS JSON) );\ncommit;\ndeclare\n\tjsonfile bfile := bfilename('LOBDIR','LC-inventory-20181019-1110.json');\n    c clob;\nbegin\n    dbms_lob.open(jsonfile);\n    dbms_lob.createtemporary(c,true);\n    dbms_lob.loadfromfile(c,jsonfile,dbms_lob.getlength(jsonfile) );\n    insert into lc_inventory_json values(c);\n    commit;\n    dbms_lob.close(jsonfile);\n    dbms_lob.freetemporary(c);\nend;\n/",
